<Alloy>
	<View id="container" onClick="focusInput">
		<View id="subView">
			<Label id="hintText" class="input"/>
			<TextField id="tfInput" class="input RegularFont" onFocus="onFocus" onChange="onChange" onReturn="onReturn" onBlur="onBlur" onClick="onClick" />
			<View id="lineBar"></View>
			<View id="mask" height="0"/>
		</View>
		<OptionDialog id="subject" onClick="clickSubject" persistent="false">
		</OptionDialog>
		<Label id="messageErorLogin"></Label>
	</View>
</Alloy>
